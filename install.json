{
	"general": {
		"name": "CRAFT based text region identificatiodn",
		"description": "Character Region Awareness for Text Detection (arXiv:1904.01941)",
		"developer": "Nicolas Dutly",
		"affiliation": "University Of Fribourg",
		"email": "ndutly@student.ethz.ch",
		"author": "Nicolas Dutly",
		"DOI": "",
		"type": "segmentation",
		"license": "MIT",
		"ownsCopyright": "0"
	},
	"input": [
		{
			"file": {
				"name": "inputImage",
				"description": "The input image from which to extract text bounding boxes",
				"options": {
					"required": true,
					"mimeTypes": {
						"allowed": [
							"image/jpeg",
							"image/png"
						],
						"default": "image/png"
					}
				}
			}
		},
		{
			"outputFolder": {}
		}
	],
	"output": [
		{
			"file": {
				"name": "Visualization",
				"type": "image",
				"description": "Bounding boyes visualization",
				"options": {
					"filename":"visual.jpg",
					"mimeTypes": {
						"allowed": [
							"image/jpeg"
						],
						"default": "image/jpeg"
					},
					"colorspace": "color",
					"visualization": true
				}
			}
		},
		{
			"file": {
				"name": "boundingboxes",
				"type": "text",
				"description": "Bounding boxes coordinates",
				"options": {
					"filename":"resbb.txt",
					"mimeTypes": {
						"allowed": [
							"text/plain"
						],
						"default": "text/plain"
					},
					"visualization": false
				}
			}
		}
	],
	"method": {
		"imageType": "docker",
		"imageName": "jumpst3r/craft-pytorch",
		"testData": "https://dl.dropbox.com/s/rddjgn5zb7qyk94/testData.zip",
		"executableType": "bash",
		"executable_path": "/input/script.sh"
	}
}
